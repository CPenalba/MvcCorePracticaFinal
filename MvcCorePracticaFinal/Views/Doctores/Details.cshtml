@model Doctor<h1 style="color:blue">Details</h1><p>    <a asp-controller="Doctores" asp-action="Index">Back to list</a></p><ul class="list-group">    <li class="list-group-item list-group-item-primary">        Hospital cod: @Model.HospitalCod    </li>    <li class="list-group-item list-group-item-danger">        Doctor no: @Model.DoctorNo    </li>    <li class="list-group-item list-group-item-info">        Apellido: @Model.Apellido    </li>    <li class="list-group-item list-group-item-success">        Especialidad: @Model.Especialidad    </li>    <li class="list-group-item list-group-item-secondary">        Salario: @Model.Salario    </li></ul>