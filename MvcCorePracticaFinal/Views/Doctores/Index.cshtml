@model List<Doctor><h1>Doctores</h1><table class="table table-bordered">    <thead>        <tr>            <th>Hospital cod</th>            <th>Doctor no</th>            <th>Apellido</th>            <th>Especialidad</th>            <th>Salario</th>            <th>Detalles</th>            <th>Borrar</th>        </tr>    </thead>    <tbody>        @foreach (Doctor d in Model)        {            <tr>                <td>@d.HospitalCod</td>                <td>@d.DoctorNo</td>                <td>@d.Apellido</td>                <td>@d.Especialidad</td>                <td>@d.Salario</td>                <td>                    <a asp-controller="Doctores" asp-action="Details" asp-route-numeroDoc="@d.DoctorNo" class="btn btn-info">                        Detalles                    </a>                </td>                <td>                    <a asp-controller="Doctores" asp-action="Delete" asp-route-numeroDoc="@d.DoctorNo" class="btn btn-danger">                        Borrar                    </a>                </td>            </tr>        }    </tbody></table>