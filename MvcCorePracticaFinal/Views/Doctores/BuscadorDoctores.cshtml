@model List<Doctor>@{    List<string> especialidades = ViewData["ESPECIALIDAD"] as List<string>;}<h1>Lambda expression Linq</h1><form method="post">    <label>Doctores: </label>    <select name="especialidad" class="form-control">        @foreach (string espe in especialidades)
        {            <option value="@espe">@espe</option>        }    </select>    <button class="btn btn-info">        Buscar doctores    </button></form>@if (Model != null)
{    <table class="table table-bordered">        <thead>            <tr>                <th>Hospital cod</th>                <th>Doctor no</th>                <th>Apellido</th>                <th>Especialidad</th>                <th>Salario</th>                           </tr>        </thead>        <tbody>            @foreach (Doctor d in Model)
            {                <tr>                    <td>@d.HospitalCod</td>                    <td>@d.DoctorNo</td>                    <td>@d.Apellido</td>                    <td>@d.Especialidad</td>                    <td>@d.Salario</td>                                    </tr>            }        </tbody>    </table>}